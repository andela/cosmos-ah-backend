{
  "name": "express-authorshaven",
  "version": "1.0.0",
  "description": "A Social platform for the creative at heart",
  "main": "index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon --exec babel-node src/index.js",
    "db-create": "sequelize db:create",
    "db-migrate": "./node_modules/.bin/babel-node ./node_modules/.bin/sequelize db:migrate",
    "db-migrate:undo": "./node_modules/.bin/babel-node ./node_modules/.bin/sequelize db:migrate:undo:all",
    "db-seed": "./node_modules/.bin/babel-node ./node_modules/.bin/sequelize db:seed:all",
    "db-refresh": "npm run db-migrate:undo && npm run db-migrate && npm run db-seed",
    "lint": "eslint ./src --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "author": "Andela Simulations Programme",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "dotenv": "^6.2.0",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.3.0",
    "passport": "^0.4.0",
    "passport-linkedin-oauth2": "^1.6.1",
    "passport-twitter": "^1.0.4",
    "pg": "^7.9.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^5.2.0",
    "sequelize-cli": "^5.4.0",
    "swagger-ui-express": "^4.0.2",
    "underscore": "^1.9.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.2",
    "babel-eslint": "^10.0.1",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-polyfill": "^6.26.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "nodemon": "^1.18.10",
    "prettier": "^1.16.4"
  }
}
